{
  "name": "bitvavo",
  "version": "1.0.0",
  "description": "Discord bot which interfaces with Bitvavo's API, bringing the latest market data to Discord.",
  "main": "dist/index",
  "types": "dist/types",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc --build tsconfig.json",
    "start": "cd ./dist/ && node index.js",
    "copy": "mkdir dist/logs && cp .env dist/.env",
    "deploy": "rm -rf dist/ && npm run-script build && npm run-script copy && npm start",
    "dev": "ts-node-dev src/index"
  },
  "keywords": [
    "discord",
    "bitvavo",
    "crypto",
    "market",
    "price"
  ],
  "author": "TheEvilSocks <TheEvilSocks@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.6.5",
    "bitvavo": "^1.2.0",
    "canvas": "^2.11.2",
    "chart.js": "^3.9.1",
    "chartjs-adapter-moment": "^1.0.0",
    "consola": "^3.2.3",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "mysql2": "^3.9.4",
    "oceanic.js": "^1.9.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "slash-create": "^5.8.0",
    "string-similarity": "^4.0.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^20.11.5",
    "@types/string-similarity": "^4.0.0",
    "@types/validator": "^13.7.5",
    "@types/ws": "^8.5.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0"
  }
}
