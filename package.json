{
  "name": "bitvavo",
  "version": "1.0.0",
  "description": "Discord bot which interfaces with Bitvavo's API, bringing the latest market data to Discord.",
  "main": "dist/index",
  "types": "dist/types",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc --build tsconfig.json",
    "start": "cd ./dist/ && node index.js",
    "copy": "mkdir dist/logs && cp .env dist/.env",
    "deploy": "rm -rf dist/ && npm run-script build && npm run-script copy && npm start",
    "dev": "ts-node-dev src/index"
  },
  "keywords": [
    "discord",
    "bitvavo",
    "crypto",
    "market",
    "price"
  ],
  "author": "TheEvilSocks <TheEvilSocks@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.26.1",
    "bitvavo": "^1.2.0",
    "canvas": "^2.9.3",
    "chart.js": "^3.9.1",
    "chartjs-adapter-moment": "^1.0.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.1",
    "eris": "^0.17.1",
    "express": "^4.17.3",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.0",
    "sequelize-typescript": "^2.1.3",
    "slash-create": "^5.8.0",
    "string-similarity": "^4.0.4",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "^16.11.19",
    "@types/string-similarity": "^4.0.0",
    "@types/validator": "^13.7.5",
    "@types/ws": "^8.5.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0"
  }
}
